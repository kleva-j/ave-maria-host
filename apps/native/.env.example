EXPO_PUBLIC_SERVER_URL=

# @effect/rpc Migration Feature Flags
# Set to 'true' to enable @effect/rpc for all endpoints
EXPO_PUBLIC_USE_EFFECT_RPC=false

# Granular endpoint migration flags
# Set individual endpoints to 'true' to migrate them to @effect/rpc
EXPO_PUBLIC_EFFECT_RPC_TODOS=false
EXPO_PUBLIC_EFFECT_RPC_AUTH=false
EXPO_PUBLIC_EFFECT_RPC_HEALTH=false

# Development and debugging flags
EXPO_PUBLIC_DEBUG_RPC=false
EXPO_PUBLIC_LOG_MIGRATION=true
